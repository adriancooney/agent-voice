{
	"name": "@agent-voice/aec",
	"version": "0.1.1",
	"description": "Acoustic echo cancellation via SpeexDSP for Node.js",
	"main": "index.js",
	"types": "index.d.ts",
	"napi": {
		"binaryName": "aec",
		"targets": [
			"aarch64-apple-darwin",
			"x86_64-apple-darwin",
			"x86_64-unknown-linux-gnu",
			"aarch64-unknown-linux-gnu"
		]
	},
	"license": "MIT",
	"devDependencies": {
		"@napi-rs/cli": "^3.0.0-alpha",
		"vitest": "^4.0.0"
	},
	"scripts": {
		"artifacts": "napi artifacts",
		"build": "napi build --platform --release --dts index.generated.d.ts",
		"build:debug": "napi build --platform",
		"check": "biome check --write --files-ignore-unknown=true __test__ vitest.config.ts",
		"prepublishOnly": "napi prepublish -t npm",
		"test": "vitest run",
		"universal": "napi universal -t @agent-voice/aec"
	},
	"files": ["index.js", "index.d.ts"],
	"optionalDependencies": {
		"@agent-voice/aec-darwin-arm64": "0.1.0",
		"@agent-voice/aec-darwin-x64": "0.1.0",
		"@agent-voice/aec-linux-x64-gnu": "0.1.0",
		"@agent-voice/aec-linux-arm64-gnu": "0.1.0"
	}
}
